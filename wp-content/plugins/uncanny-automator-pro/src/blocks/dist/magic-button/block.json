{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "uncanny-automator/magic-button",
  "version": "0.0.1",
  "title": "Magic Button",
  "category": "uncanny-automator",
  "textdomain": "uncanny-automator-pro",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "render": "file:./render.php",
  "attributes": {
    "id": {
      "type": "integer",
      "default": 0
    },
    "is_ajax": {
      "type": "string",
      "default": "no"
    },
    "label": {
      "type": "string",
      "default": "Click here"
    },
    "success_message": {
      "type": "string",
      "default": "Done"
    },
    "submit_message": {
      "type": "string",
      "default": "Processing..."
    }
  },
  "keywords": [
    "Automator",
    "Uncanny Automator",
    "Magic Button",
    "Recipe Trigger"
  ]
}